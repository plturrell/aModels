CREATE VIEW `vw_recon_stg_t_cdr` AS select 
`f`.`w_snapshot_dt`,
`f`.`w_datasource_id`,
`f`.`w_country_cd`,
`f`.`w_batch_id`,
`jb`.`regime` `workspace`,
`jb`.`ctry_cd` `batch_ctry_cd`,
`jb`.`src_sys_nm` `batch_src_sys_nm`,
`jb`.`reporting_dt` `batch_reporting_dt`,
`f`.`dealbook`
,sum(CASE WHEN `f`.`table_name` NOT IN ('CREDIT_DERIVATIVE', 'DEAL_IMPORT', 'FACILITY') THEN NVL(`f`.`outstanding`, 0) ELSE 0 END) `outstanding`,sum(`f`.`rwa`) `rwa`,sum(`f`.`ead`) `ead`,count(distinct `f`.`counterparty_original`) `cnt_counterparty_original`,count(distinct `f`.`counterparty_new`) `cnt_counterparty_new`
,count(distinct `f`.`contract_reference`) `cnt_contract_reference` ,
count(*) `cnt`
from `sgmisitstg`.`stg_bcrs_t_cdr` `f`
join 
(select `a`.`batch_id`, `a`.`ctry_cd`, `a`.`src_sys_nm`, `a`.`reporting_dt`, `a`.`prev_reporting_dt`, `a`.`daily_monthly_flag`, `a`.`regime`, `a`.`status`, `a`.`pstg_status`, `a`.`stg_status`, `a`.`fnd_status`, `a`.`plp_status`, `a`.`out_status`, `a`.`outbound_status`, `a`.`start_ts`, `a`.`end_ts`, `a`.`pstg_start_ts`, `a`.`pstg_end_ts`, `a`.`stg_start_ts`, `a`.`stg_end_ts`, `a`.`fnd_start_ts`, `a`.`fnd_end_ts`, `a`.`plp_start_ts`, `a`.`plp_end_ts`, `a`.`out_start_ts`, `a`.`out_end_ts`, `a`.`outbound_start_ts`, `a`.`outbound_end_ts`, `a`.`message`, `a`.`max_batch_id` from (select `b`.`batch_id`, `b`.`ctry_cd`, `b`.`src_sys_nm`, `b`.`reporting_dt`, `b`.`prev_reporting_dt`, `b`.`daily_monthly_flag`, `b`.`regime`, `b`.`status`, `b`.`pstg_status`, `b`.`stg_status`, `b`.`fnd_status`, `b`.`plp_status`, `b`.`out_status`, `b`.`outbound_status`, `b`.`start_ts`, `b`.`end_ts`, `b`.`pstg_start_ts`, `b`.`pstg_end_ts`, `b`.`stg_start_ts`, `b`.`stg_end_ts`, `b`.`fnd_start_ts`, `b`.`fnd_end_ts`, `b`.`plp_start_ts`, `b`.`plp_end_ts`, `b`.`out_start_ts`, `b`.`out_end_ts`, `b`.`outbound_start_ts`, `b`.`outbound_end_ts`, `b`.`message`,max(case when `b`.`stg_status` = 'SUCCESS' and `b`.`status` <> 'FAILED' then `b`.`batch_id` end) over (partition by `b`.`ctry_cd`,`b`.`src_sys_nm`,`b`.`reporting_dt`,`b`.`regime`,`b`.`daily_monthly_flag`) `max_batch_id` from `sgmisitetl`.`jbs_batch` `b`) `a` where `a`.`max_batch_id` = `a`.`batch_id`) `jb` where `f`.`w_batch_id` = `jb`.`batch_id`
group by 
