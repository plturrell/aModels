CREATE VIEW `vw_recon_edm` AS select `stg_edm_scorecard`.`w_snapshot_dt` ,'STG' `layer`, 'SCORECARD' `recon_item`, count (distinct `stg_edm_scorecard`.`cust_id`) `CLIENT_COUNT` , 0 `lmt_amt`
from `sgmisitstg`.`stg_edm_scorecard`
group by `stg_edm_scorecard`.`w_snapshot_dt`
union all
select `sgmi_client_scorecard_d`.`w_snapshot_dt` ,'FND'  , 'SCORECARD' , count (distinct `sgmi_client_scorecard_d`.`client_le_id`) , 0  
from `sgmisit`.`sgmi_client_scorecard_d`
group by `sgmi_client_scorecard_d`.`w_snapshot_dt`
union all
select `stg_edm_limit`.`w_snapshot_dt` ,'STG'  , 'LIMIT' , count (distinct `stg_edm_limit`.`lmt_le_id`) , sum(cast (`stg_edm_limit`.`lmt_amt` as double))
from `sgmisitstg`.`stg_edm_limit`
group by `stg_edm_limit`.`w_snapshot_dt`
union all
select `sgmi_client_limit_f`.`w_snapshot_dt` ,'FND'  , 'LIMIT' , count (distinct `sgmi_client_limit_f`.`edm_client_le_wid`), sum(`sgmi_client_limit_f`.`edm_approved_limit_amount`)
from `sgmisit`.`sgmi_client_limit_f`
where `sgmi_client_limit_f`.`w_datasource_id` = 'BCRS_PRA'
group by `sgmi_client_limit_f`.`w_snapshot_dt`
