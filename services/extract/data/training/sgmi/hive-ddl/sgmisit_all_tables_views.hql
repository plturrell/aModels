      , sum(cast(regexp_replace(`stg_edm_groupmandate`.`prop_lmt_cat1_others`,',','''') as decimal(20,2))) `cat1_other`
      , sum(cast(regexp_replace(`stg_edm_groupmandate`.`prop_lmt_cat2_st`,',','''') as decimal(20,2))) `cat2_st`
      , sum(cast(regexp_replace(`stg_edm_groupmandate`.`prop_lmt_cat2_lt`,',','''') as decimal(20,2))) `cat2_lt`
      , sum(cast(regexp_replace(`stg_edm_groupmandate`.`prop_lmt_cat3`,',','''') as decimal(20,2))) `cat3_settlement_limits`
      , sum(cast(regexp_replace(`stg_edm_groupmandate`.`prop_lmt_ctrllmt`,',','''') as decimal(20,2))) `control_limits`
      , sum(cast(regexp_replace(`stg_edm_groupmandate`.`prop_lmt_cat5`,',','''') as decimal(20,2))) `cat5_limits`
  from `sgmisitstg`.`stg_edm_groupmandate` 
  group by `stg_edm_groupmandate`.`w_snapshot_dt`
  UNION ALL
    select `sgmi_gm_tde_f`.`w_snapshot_dt` , 'FND' , count(distinct `sgmi_gm_tde_f`.`client_group_id`)        
      , sum(`sgmi_gm_tde_f`.`cat1_lt_lending`)  
      , sum(`sgmi_gm_tde_f`.`cat1_trade`)  
      , sum(`sgmi_gm_tde_f`.`cat1_other`) 
      , sum(`sgmi_gm_tde_f`.`cat2_st`) 
      , sum(`sgmi_gm_tde_f`.`cat2_lt`) 
      , sum(`sgmi_gm_tde_f`.`cat3_settlement_limits`) 
      , sum(`sgmi_gm_tde_f`.`control_limits`) 
      , sum(`sgmi_gm_tde_f`.`cat5_limits`) 
  from `sgmisit`.`sgmi_gm_tde_f` 
  where `sgmi_gm_tde_f`.`src_data` = 'EDM'
  group by `sgmi_gm_tde_f`.`w_snapshot_dt`
CREATE VIEW `vw_recon_mfu` AS select `stg_mfu_gsam_customers`.`w_snapshot_dt` ,'STG' `layer`, 'GSAM' `recon_item`, count (distinct `stg_mfu_gsam_customers`.`leid`) `CLIENT_COUNT`  
from `sgmisitstg`.`stg_mfu_gsam_customers`
group by `stg_mfu_gsam_customers`.`w_snapshot_dt`
union all
select `sgmi_gsam_customer_d`.`w_snapshot_dt` ,'FND'  , 'GSAM' , count (distinct `sgmi_gsam_customer_d`.`leid`) 
from `sgmisit`.`sgmi_gsam_customer_d`
where `sgmi_gsam_customer_d`.`src_mfu` = 'Y'
group by `sgmi_gsam_customer_d`.`w_snapshot_dt`
union all
select `d`.`w_snapshot_dt` ,'STG'  , 'EAR' , count (distinct `d`.`client_id`) 
from `sgmisit`.`sgmi_client_ext_d` `d` join `sgmisitstg`.`stg_mfu_pvb_ear`  `s`
                        on `d`.`w_snapshot_dt` = `s`.`w_snapshot_dt`
where `d`.`src_mfu` = 'Y' and  `d`.`credit_status_description` is not  null
and `d`.`client_id` like CONCAT('%', `s`.`cif_numbers`, '%') 
group by `d`.`w_snapshot_dt`
union all
select `sgmi_client_ext_d`.`w_snapshot_dt` ,'FND'  , 'EAR' , count (distinct `sgmi_client_ext_d`.`client_id`) 
from `sgmisit`.`sgmi_client_ext_d`
where `sgmi_client_ext_d`.`src_mfu` = 'Y' and  `sgmi_client_ext_d`.`credit_status_description` is not  null
group by `sgmi_client_ext_d`.`w_snapshot_dt`
CREATE VIEW `vw_recon_rmi` AS select 'STG' `layer`, `stg_rmi_ext_ear`.`w_snapshot_dt`, 'EAR' `recon_item` , count( distinct `stg_rmi_ext_ear`.`le_id` )  `client_count`
from `sgmisitstg`.`stg_rmi_ext_ear` 
where `stg_rmi_ext_ear`.`w_datasource_id` = 'RMI'
group by `stg_rmi_ext_ear`.`w_snapshot_dt`
union all 
select 'STG', `stg_rmi_gsam_customers`.`w_snapshot_dt`, 'GSAM'  , count( distinct `stg_rmi_gsam_customers`.`leid` )  
from `sgmisitstg`.`stg_rmi_gsam_customers` 
where `stg_rmi_gsam_customers`.`w_datasource_id` = 'RMI'
group by `stg_rmi_gsam_customers`.`w_snapshot_dt`
union all 
select 'STG', `stg_rmi_policy`.`w_snapshot_dt`  , 'PE' , count( distinct `stg_rmi_policy`.`counterparty` ) 
from `sgmisitstg`.`stg_rmi_policy` 
where `stg_rmi_policy`.`w_datasource_id` = 'RMI'
group by `stg_rmi_policy`.`w_snapshot_dt`
union all 
select 'FND', `sgmi_client_ext_d`.`w_snapshot_dt`  , 'EAR' , count( distinct `sgmi_client_ext_d`.`client_id` ) 
from `sgmisit`.`sgmi_client_ext_d` 
where `sgmi_client_ext_d`.`w_datasource_id` = 'RMI'
and `sgmi_client_ext_d`.`src_rmi` = 'Y' and `sgmi_client_ext_d`.`credit_status_description` is not  null
group by `sgmi_client_ext_d`.`w_snapshot_dt`
union all 
select 'FND', `sgmi_gsam_customer_d`.`w_snapshot_dt`  , 'GSAM' , count( distinct `sgmi_gsam_customer_d`.`leid` ) 
from `sgmisit`.`sgmi_gsam_customer_d` 
where `sgmi_gsam_customer_d`.`w_datasource_id` = 'RMI'
and `sgmi_gsam_customer_d`.`src_rmi` = 'Y'
group by `sgmi_gsam_customer_d`.`w_snapshot_dt`
union all 
select 'FND', `sgmi_client_ext_d`.`w_snapshot_dt`  , 'PE' , count( distinct `sgmi_client_ext_d`.`client_id` ) 
from `sgmisit`.`sgmi_client_ext_d` 
where `sgmi_client_ext_d`.`w_datasource_id` = 'RMI'
and `sgmi_client_ext_d`.`pe_code` is not null
group by `sgmi_client_ext_d`.`w_snapshot_dt`
