    {
      "id": "rule_translate_en_ar",
      "name": "English to Arabic Translation Routing",
      "description": "Route English to Arabic translation requests to MT domain",
      "priority": 100,
      "conditions": [
        {
          "type": "path",
          "field": "path",
          "operator": "equals",
          "value": "/v1/translate/en-ar",
          "negate": false,
          "description": "Request path is /v1/translate/en-ar"
        }
      ],
      "actions": [
        {
          "type": "route_to_domain",
          "target": "mt-en-ar",
          "parameters": {
            "reason": "English to Arabic translation request"
          },
          "description": "Route to English to Arabic translation domain"
        }
      ],
      "enabled": true,
      "created_at": "2024-01-15T10:00:00Z",
