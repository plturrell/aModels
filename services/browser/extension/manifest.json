{
  "manifest_version": 3,
  "name": "aModels Tools",
  "version": "0.0.1",
  "description": "Layer4 tools in your browser via the unified gateway.",
  "action": {
    "default_title": "aModels",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": ["storage"],
  "host_permissions": [
    "http://localhost/*",
    "https://*/"
  ]
}


