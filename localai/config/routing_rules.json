    }
  ],
  "default_domain": "general",
  "fallback_domain": "general",
  "load_balancing": {
    "enabled": true,
    "strategy": "weighted",
    "health_check": true,
    "max_retries": 3,
    "retry_delay_ms": 1000,
    "circuit_breaker": true,
    "failure_threshold": 0.5
  },
  "performance": {
    "cache_enabled": true,
    "cache_ttl": "1h",
    "max_concurrency": 100,
    "request_timeout": "1m",
    "response_timeout": "2m",
    "rate_limit_enabled": true,
    "rate_limit_rps": 100
  },
  "monitoring": {
    "enabled": true,
    "metrics_enabled": true,
    "logging_enabled": true,
    "alerting_enabled": true,
    "metrics_interval": "1m",
    "log_level": "info",
    "alert_thresholds": {
      "error_rate": 0.05,
      "latency": 5000,
      "throughput": 1000
    }
  },
  "version": "1.0.0",
  "last_updated": "2024-01-15T10:00:00Z"
}
