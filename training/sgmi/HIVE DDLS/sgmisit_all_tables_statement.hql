CREATE VIEW `vw_recon_mfu` AS select `stg_mfu_gsam_customers`.`w_snapshot_dt` ,'STG' `layer`, 'GSAM' `recon_item`, count (distinct `stg_mfu_gsam_customers`.`leid`) `CLIENT_COUNT`  
