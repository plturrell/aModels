group by `stg_mfu_gsam_customers`.`w_snapshot_dt`
union all
select `sgmi_gsam_customer_d`.`w_snapshot_dt` ,'FND'  , 'GSAM' , count (distinct `sgmi_gsam_customer_d`.`leid`) 
from `sgmisit`.`sgmi_gsam_customer_d`
